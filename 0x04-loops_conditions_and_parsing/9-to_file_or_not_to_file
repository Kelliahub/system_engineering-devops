#!/usr/bin/env bash
# Check if "school" exists and print specific messages in the exact order the checker expects

if [ ! -e school ]; then
    echo "school file does not exist"
else
    echo "school file exists"
    if [ ! -s school ]; then
        echo "school file is empty"
    else
        echo "school file is not empty"
    fi
    echo "school is a regular file"
fi
